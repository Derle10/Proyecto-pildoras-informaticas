
public class Uso_arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int matriz[] = new int [5];
		
		matriz[0] = 5;
		matriz[1] = 10;
		matriz[2] = 15;
		matriz[3] = 5;
		matriz[4] = 20;
		
		for ( int i = 0; i<5 ; i++){
			
			System.out.println("El valor del indice es " + i + "=" + matriz[i]);
		}

	}

}
